{
  "name": "@antdvr/vue-template-3.x",
  "description": "Vue3.x - 基于 Antdv 组件库后台管理系统模版",
  "homepage": "https://github.com/antdvr/vue-template-3.x",
  "author": "lin pengteng <china.linpengteng@gmail.com>",
  "license": "MIT License",
  "version": "4.0.0",
  "keywords": [
    "vue-project-template",
    "vue-admin-template",
    "ant-design-vue-pro",
    "antdv-pro"
  ],
  "scripts": {
    "dev": "vite",
    "test": "vitest --ui",
    "cypress": "cypress open --component",
    "build": "vue-tsc --noEmit && vite build",
    "eslint": "npx eslint --fix --quiet src/**/*{.vue,.tsx,.ts}",
    "release": "release-it",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx husky install",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@antdvr/library-3.x": "^4.0.1",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.10.0",
    "dayjs": "^1.11.13",
    "js-simpler": "^1.7.0",
    "md5": "^2.3.0",
    "msw": "^2.10.2",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persist": "^1.0.0",
    "qs": "^6.14.0",
    "vue": "^3.5.17",
    "vue-ls": "^4.2.0",
    "vue-router": "^4.5.1",
    "vue-types": "^6.0.0"
  },
  "devDependencies": {
    "@antdvr/eslint-config-3.x": "^4.0.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/config-inspector": "^1.1.0",
    "@eslint/js": "^9.30.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@stylistic/eslint-plugin": "^5.0.0",
    "@types/md5": "^2.3.5",
    "@types/node": "^24.0.7",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.14.0",
    "@types/vue-ls": "^3.2.7",
    "@vitejs/plugin-legacy": "^7.0.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "cypress": "^14.5.0",
    "cz-message-helper": "^2.0.0",
    "darkreader": "^4.9.105",
    "eslint": "^9.30.0",
    "eslint-plugin-vue": "^10.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "less": "^4.3.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "release-it": "^19.0.3",
    "rollup": "^4.44.1",
    "terser": "^5.43.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^7.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^3.2.4",
    "volar-service-prettyhtml": "^0.0.64",
    "vue-tsc": "^2.2.10"
  },
  "config": {
    "cz-message-helper": {
      "config": ".cz-message.cjs"
    },
    "commitizen": {
      "path": "node_modules/cz-message-helper"
    }
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.12.3"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "packageManager": "pnpm@9.12.3"
